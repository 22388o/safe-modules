{
  "name": "@gnosis.pm/safe-module-whitelist",
  "version": "0.1.0",
  "description": "Modules for the Gnosis Safe contracts",
  "license": "GPL-3.0",
  "main": "index.js",
  "files": [
    "contracts",
    "test",
    "build"
  ],
  "scripts": {
    "test": "run(){ run-with-testrpc -l 20000000 --noVMErrorsOnRPCResponse \"truffle test $@\"; }; run",
    "preversion": "yarn restore",
    "restore": "yarn truffle compile && yarn tnt cB",
    "deploy": "run(){ truffle deploy --network=$@; }; run",
    "do": "run(){ truffle exec --network=$@; }; run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosis/safe-modules.git"
  },
  "keywords": [
    "Ethereum",
    "Safe"
  ],
  "author": "richard@gnosis.pm",
  "bugs": {
    "url": "https://github.com/gnosis/safe-modules/issues"
  },
  "devDependencies": {
    "@digix/tempo": "^0.2.0",
    "@gnosis.pm/mock-contract": "3.0.8",
    "@gnosis.pm/truffle-nice-tools": "^1.3.0",
    "eth-lightwallet": "^4.0.0",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^6.2.0",
    "ganache-cli": "6.3.0",
    "random-buffer": "*",
    "run-with-testrpc": "^0.3.0",
    "solidity-coverage": "^0.7.4",
    "web3": "^1.2.6",
    "solium": "^1.2.1"
  },
  "dependencies": {
    "@gnosis.pm/safe-contracts": "^1.1.1-dev.2",
